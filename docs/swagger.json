        "/auth/upload-photo": {
            "post": {
                "description": "Upload photo with user id from context and form file",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "Upload photo",
                "parameters": [
