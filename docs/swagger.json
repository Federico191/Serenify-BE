    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/auth/current-user": {
            "get": {
                "description": "Get current user with user id from context",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "Get current user",
                "responses": {
                    "200": {
                        "description": "OK",
